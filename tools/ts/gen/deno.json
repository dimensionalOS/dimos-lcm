{
  "name": "@dimos/lcm-ts",
  "version": "0.1.0",
  "exports": {
    ".": "./mod.ts",
    "./parser": "./parser.ts",
    "./generator": "./generator.ts",
    "./types": "./types.ts"
  },
  "tasks": {
